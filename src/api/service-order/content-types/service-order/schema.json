{
  "kind": "collectionType",
  "collectionName": "service_orders",
  "info": {
    "singularName": "service-order",
    "pluralName": "service-orders",
    "displayName": "ServiceOrder",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "date": {
      "type": "date"
    },
    "user": {
      "type": "component",
      "repeatable": false,
      "component": "order.user"
    },
    "details": {
      "type": "boolean",
      "default": false
    },
    "workTime": {
      "type": "component",
      "repeatable": false,
      "component": "order.time-range"
    },
    "address": {
      "type": "text",
      "maxLength": 240
    },
    "note": {
      "type": "text",
      "maxLength": 500,
      "minLength": 5
    }
  }
}
