{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Товары",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 2,
      "maxLength": 50
    },
    "description": {
      "type": "richtext",
      "required": true,
      "minLength": 10
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "gasoline",
        "bitumen",
        "heating"
      ],
      "required": true
    }
  }
}
